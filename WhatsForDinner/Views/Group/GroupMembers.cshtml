@model List<AspNetUsers>
@{
    ViewData["Title"] = "Group Members";
}

<h1 align="center">@TempData["group"].ToString()</h1>

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th>Group Members</th>
            <th>Email</th>
        </tr>
    </thead>
        @foreach (AspNetUsers user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
            </tr>
        }
</table>